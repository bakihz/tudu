{
  "name": "tudu",
  "version": "1.0.0",
  "description": "tudu",
  "license": "MIT",
  "author": "Bakihz",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-rebuild",
    "package": "electron-packager . Tudu --platform=win32 --arch=x64 --out=dist --overwrite --icon=icon.ico"
  },
  "build": {
    "appId": "com.haft.tudu",
    "productName": "Tudu",
    "copyright": "Copyright Â© 2025 Bakihz",
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "icon": "./icon.ico",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "auto-launch": "^5.0.6",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "git": "^0.1.5",
    "mssql": "^11.0.1",
    "pureimage": "^0.4.18",
    "telnet": "^0.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakihz/tudu.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/bakihz/tudu/issues"
  },
  "homepage": "https://github.com/bakihz/tudu#readme"
}
